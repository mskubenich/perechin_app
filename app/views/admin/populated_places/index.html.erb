<br/><table class="data-list" cellpadding="0" cellspacing="0">
  <% @places.each do |place|%>
      <tr>
        <td>
          <%= link_to place.title, maps_populated_place_path(:id => place.id) %>
        </td>
        <td>
          <%= link_to "Edit", edit_admin_populated_place_path(place), :class => "button submit_button" %>
        </td>
        <td>
          <%= link_to "Delete", admin_populated_place_path(place), :confirm => "Are you sure?", :method => :delete, :class => "button remove_button"  %>
        </td>
      </tr>
  <% end %>
</table>


<%= content_for :sidebar do %>
    <%= render :partial => 'sidebar', :locals => {:art_categories => @art_categories, :category => @category}%>
<% end %>